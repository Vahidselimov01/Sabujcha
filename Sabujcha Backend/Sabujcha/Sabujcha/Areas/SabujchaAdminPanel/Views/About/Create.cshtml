@using Sabujcha.Models;
@model AboutUsArea
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Name" class="control-label"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Description" class="control-label"></label>
        <input asp-for="Description" class="form-control" />
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Title" class="control-label"></label>
        <input asp-for="Title" class="form-control" />
        <span asp-validation-for="Title" class="text-danger"></span>
    </div>
   
    <div class="form-group">
        <label asp-for="FormFile" class="control-label"></label>
        <input asp-for="FormFile" accept="image/*" class="form-control" />
        <span asp-validation-for="FormFile" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="FormFile2" class="control-label"></label>
        <input asp-for="FormFile2" accept="image/*" class="form-control" />
        <span asp-validation-for="FormFile2" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Ekle" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-secondary">Geriye Dön</a>
    </div>
</form>